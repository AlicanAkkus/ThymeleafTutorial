<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
	<display-name>ThymeleafTutorial</display-name>
	
	
	<listener>
		<listener-class>com.wora.servlet.TemplateInitializer</listener-class>
	</listener>
	
	<servlet>
		<servlet-name>ThyHello</servlet-name>
		<servlet-class>com.wora.servlet.ThymeleafHello</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ThyHello</servlet-name>
		<url-pattern>*.html</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddOrders</servlet-name>
		<servlet-class>com.wora.servlet.AddOrderServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddOrders</servlet-name>
		<url-pattern>/addOrder</url-pattern>
	</servlet-mapping>
	
	

</web-app>